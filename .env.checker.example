# Remote Checker Configuration
# Copy this file to .env.checker and fill in the values

# URL of the central Hosh web API
WEB_API_URL=https://hosh.zec.rocks

# API key for authentication (must match the central instance)
API_KEY=your_api_key_here

# Geographic location identifier for this checker (IATA airport code)
CHECKER_LOCATION=fra

# SOCKS proxy for .onion addresses (required for Tor hidden service checks)
SOCKS_PROXY=tor:9050
